{
	"name": "swiftdocs",
	"displayName": "SwiftDocs",
	"description": "A set of tools to help you understand your code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:collectionExplorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "collectionExplorer",
					"name": "SwiftDocs: Collections"
				}
			]
		},
		"commands": [
			{
				"command": "collectionExplorer.addCollection",
				"title": "New Collection"
			},
			{
				"command": "extension.assignFile",
				"title": "Assign file to Collection"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.assignFile",
					"group": "navigation@1"
				}
			],
			"view/title": [
				{
					"command": "collectionExplorer.addCollection",
					"when": "view == collectionExplorer",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
